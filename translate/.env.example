# S3 Configuration
# Copy this file to .env and fill in your actual values

# S3 Access Key
S3_ACCESS_KEY=your_access_key_here

# S3 Secret Key  
S3_SECRET_KEY=your_secret_key_here

# S3 Endpoint URL (leave empty for AWS S3 default)
S3_URL=https://s3.amazonaws.com

# SSL Verification (true/false)
# Set to false for self-signed certificates or local MinIO
SSL_VERIFY=true

# For MinIO, use something like: http://localhost:9000
# For AWS S3, use: https://s3.amazonaws.com or leave empty
